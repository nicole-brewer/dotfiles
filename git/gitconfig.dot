# This is Git's per-user configuration file.
[user]
    name = Nicole Brewer
    email = brewer36@purdue.edu
[push]
    default = simple
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = true
    pager = true
# also use the local gitconfig if there is one
[include]
    path = ~/.gitconfig.local
[credential "https://github.com"]
	helper = !/opt/homebrew/bin/gh auth git-credential
